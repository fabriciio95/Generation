package exemplos;

public class OiMundo {
	
	public static void main(String[] args) {
		
		int numero1 = 10;
		int numero2 = 20;
		
		System.out.println("Olá Mundo!!!");
		System.out.println("Fabricio Siqueira Macedo");
		System.out.printf("%d + %d = %d", numero1, numero2, numero1 + numero2);
		
	}

}
